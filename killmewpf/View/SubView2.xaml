<UserControl
    x:Class="killmewpf.View.SubView2"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:v="clr-namespace:killmewpf.View"
    xmlns:vm="clr-namespace:killmewpf.ViewModel"
    d:DataContext="{d:DesignInstance Type=vm:SubViewModel2,
                                     IsDesignTimeCreatable=True}"
    d:DesignHeight="50"
    d:DesignWidth="100"
    mc:Ignorable="d">
    <StackPanel>
        <TextBlock Foreground="Blue" Text="{Binding Path=Title}" />
    </StackPanel>
</UserControl>
